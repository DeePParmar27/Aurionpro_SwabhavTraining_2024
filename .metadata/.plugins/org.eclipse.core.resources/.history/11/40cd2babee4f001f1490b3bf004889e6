package com.aurionpro.structural.adaptar.test;

public class Biscuit implements IItems{
	
	
	private String name;
	private int price;
	
	

	public Biscuit(String name, int price) {
		super();
		this.name = name;
		this.price = price;
	}

	@Override
	public String getItemName() {
		return this.name;
		
	}

	@Override
	public int getItemPrice() {
		return this.price;
		
	}

}
