package com.aurionpro.model;

import static org.junit.jupiter.api.Assertions.assertEquals;

import org.junit.jupiter.api.BeforeEach;
import org.junit.jupiter.api.Test;

public class AreaCalculationTest {
	
	AreaCalculator calculator ;
	
	@BeforeEach
	void init() {
		calculator = new AreaCalculator() ;
	}
	
	@Test
	public void AreaOfCircleTest(int radius) {
		assertEquals(78.5, calculator.areaOfCircle(5));
	}
	
	@Test
	public void AreaOfSquareTest(int height , int breadth) {
		assertEquals(144, calculator.areaOfRectangle(12, 12));
	}

}
