package com.aurionpro.dip.violation.model;

public class TaxCalculator {
	
	DBlogger logger ;
	private int amount ;
	private int rate ;
	public int getAmount() {
		return amount;
	}
	public void setAmount(int amount) {
		this.amount = amount;
	}
	public int getRate() {
		return rate;
	}
	public void setRate(int rate) {
		this.rate = rate;
	}
	public TaxCalculator( int amount, int rate) {
		this.amount = amount;
		this.rate = rate;
	}
	
	int tax = 0;
			
	try {
		tax = amount/rate ;
		System.out.println(tax);
	}catch(Exception e) {
		new DBlogger().log("Divide By Zero");
	}
	
	
	

}
