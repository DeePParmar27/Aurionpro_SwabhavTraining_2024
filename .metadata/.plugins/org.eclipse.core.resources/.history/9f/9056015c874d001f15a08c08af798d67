package com.aurionpro.test;

import java.util.Arrays;
import java.util.List;
import java.util.stream.Collectors;
import java.util.stream.Stream;

public class Test {
	public static void main(String[] args) {
		
		//--Operation Performed on stream does'nt modefy the source
		List<Integer> numbers = Arrays.asList(10,20,30,46,55,85,79);
		Stream<Integer> numberStream = numbers.stream();
		
		// numberStream.forEach((n)->System.out.print(n+" "));
		
//		List<Integer> FilteredNumbers = numbers.stream().filter((n)->(n%2 != 0))
//		             .filter((n)-> (n>35))
//		             .collect(Collectors.toList());
		
		
		List<Integer> squareNumbers = numberStream.map((n)->n*n).collect(Collectors.toList());
		
		squareNumbers.forEach((number)->System.out.println(numbers));
		            
		
	}
	
}
