public class shoppingTest {
	public static void main(String[] args) {
		Hat h1 = new Hat("Nike", "Nike-Doreman", 2100, 90);
		Hat h2 = new Hat("Urban", "Urban-Go", 900, 90);
		Hat h3 = new Hat("Wrong", "Wrong-Whale", 00, 90);
		
