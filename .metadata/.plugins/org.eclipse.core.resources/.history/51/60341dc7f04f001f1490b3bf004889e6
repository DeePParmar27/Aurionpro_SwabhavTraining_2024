		sh.addItemsTocart((IItems) new Hat("Nike", "Nike-Doreman", 2100, 90));
		sh.addItemsTocart(new Biscuit("Parle-G", 1000));
		sh.addItemsTocart((IItems) new Hat("Urban", "Urban-Go", 900, 90));

	}

}
